<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net4.8;netstandard2.1;netcoreapp3.1;net6.0</TargetFrameworks>
		<RootNamespace>MDbContext</RootNamespace>
		<AssemblyName>MT.LightORM</AssemblyName>
		<Authors>MarvelTiter</Authors>
		<Nullable>enable</Nullable>
		<LangVersion>10.0</LangVersion>
		<Description>使用表达式树（ExpressionTree）构建Sql，仅用于学习</Description>
		<Version>1.1.8.7</Version>
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/MarvelTiter/LightORM.git</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<FileVersion>1.1.8.7</FileVersion>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="ExpressionSql\ExpressionSqlCore.cs" />
	  <Compile Remove="ExpressionSql\ExpressionSqlCore.Handle.cs" />
	  <Compile Remove="ExpressionSql\ExpressionSqlCore.Result.cs" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="ExpressionSql\Interface\Select\select.js">
	    <SubType>Code</SubType>
	  </None>
	</ItemGroup>

</Project>
